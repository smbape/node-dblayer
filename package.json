{
  "name": "dblayer",
  "version": "0.1.4",
  "description": "Database layer",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "prepublish": "coffee -bo lib/ src/",
    "postinstall": "node -e \"require('fs').readdir('lib',function(e){e&&require('child_process').exec('npm run prepublish')})\"",
    "test": "node test/_mocha --full-trace --compilers coffee:coffee-script/register test/prepare.coffee test/suite",
    "test-postgres": "node -e \"require('anyspawn').exec('node node_modules/mocha/bin/_mocha --full-trace --compilers coffee:coffee-script/register test/prepare.coffee test/suite', {stdio: 'inherit', env: {DIALECT: 'postgres'}})\"",
    "test-mysql": "node -e \"require('anyspawn').exec('node node_modules/mocha/bin/_mocha --full-trace --compilers coffee:coffee-script/register test/prepare.coffee test/suite', {stdio: 'inherit', env: {DIALECT: 'mysql'}})\"",
    "test-cover": "node node_modules/istanbul/lib/cli.js --dir=test/coverage cover test/_mocha -- --full-trace --compilers coffee:coffee-script/register test/prepare.coffee test/suite"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/smbape/node-dblayer"
  },
  "author": "St√©phane MBAPE",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/smbape/node-dblayer/issues"
  },
  "homepage": "https://github.com/smbape/node-dblayer",
  "readmeFilename": "readme.md",
  "dependencies": {
    "async": "^2.0.0-rc.5",
    "backbone": "^1.3.3",
    "duplex-arraystream": "^0.0.1",
    "lodash": "^4.12.0",
    "log4js": "^0.6.36",
    "lru-cache": "^4.0.1",
    "moment": "^2.13.0",
    "mysql": "^2.8.0",
    "once": "^1.3.2",
    "pg": "^4.5.5",
    "pg-native": "^1.10.0",
    "pg-query-stream": "^0.7.0",
    "prepend-listener": "0.0.0",
    "sem-lib": "^1.0.0",
    "sqlite3": "^3.1.4",
    "squel": "^4.4.2"
  },
  "devDependencies": {
    "anyspawn": "^1.0.8",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "coffee-script": "^1.10.0",
    "commander": "^2.8.1",
    "istanbul": "^0.4.3",
    "jshint": "^2.9.2",
    "knex": "^0.11.3",
    "mkdirp": "^0.5.1",
    "mocha": "^2.4.5",
    "nodeunit": "^0.9.1",
    "rimraf": "^2.5.2",
    "sql": "^0.52.0",
    "temp": "^0.8.3"
  }
}
